{
    "name": "dot-anything",
    "displayName": "Dot Anything",
    "description": "Press the dot key to transform into anything.",
    "publisher": "lqzh",
    "author": {
        "name": "lqzh"
    },
    "version": "0.0.1",
    "icon": "public/logo.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.103.0"
    },
    "categories": [
        "Snippets",
        "Formatters"
    ],
    "keywords": [
        "Snippets",
        "Formatters",
        "convert",
        "transform",
        "template"
    ],
    "pricing": "Free",
    "activationEvents": [
        "onStartupFinished"
    ],
    "sponsor": {
        "url": "https://github.com/sponsors/lqzhgood"
    },
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "dot-anything.helloWorld",
                "title": "Hello World"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "workbench.action.reloadWindow",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "npm run check-types && npm run lint && node esbuild.js",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "package": "npm run check-types && npm run lint && node esbuild.js --production",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "check-types": "tsc --noEmit",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.103.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.32.0",
        "esbuild": "^0.25.8",
        "npm-run-all": "^4.1.5",
        "typescript": "^5.9.2",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/lqzhgood/vscode-ext-dot-anything/issues",
        "email": "sean@contoso.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lqzhgood/vscode-ext-dot-anything.git"
    },
    "homepage": "https://github.com/lqzhgood/vscode-ext-dot-anything/blob/main/README.md"
}
